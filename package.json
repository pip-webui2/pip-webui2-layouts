{
  "name": "pip-webui2-layouts",
  "version": "2.0.0-rc.5",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "pip-webui2-themes build --soft && ng serve",
    "build": "pip-webui2-themes build && ng build",
    "build:lib": "ng build pip-webui2-layouts --prod && npm run build:lib-styles && npm run build:lib-md",
    "build:lib-styles": "scss-bundle -c ./projects/pip-webui2-layouts/scss-bundle.config.json",
    "build:lib-md": "cpx \"{LICENSE,README.md,CHANGELOG.md}\" dist/pip-webui2-layouts",
    "test": "ng test",
    "test:lib": "ng test pip-webui2-layouts",
    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui2-layouts",
    "e2e": "ng e2e",
    "package": "npm run build:lib && cd dist/pip-webui2-layouts && npm pack",
    "deploy": "ng build --prod --base-href https://pip-webui2.github.io/pip-webui2-layouts/ && ngh --dir dist/pip-webui2-layouts-app"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "core-js": "^3.6.5",
    "css-element-queries": "^1.2.3",
    "lodash": "^4.17.20",
    "masonry-layout": "^4.2.2",
    "node-sass": "^4.14.1",
    "pip-webui2-themes": "^2.0.0-rc.3",
    "rxjs": "~6.6.3",
    "scss-bundle": "^3.1.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@angular/language-service": "^10.1.4",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.2",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
